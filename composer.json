{
  "name": "drupal/bento_sdk",
  "description": "Official Bento API integration for Drupal 10",
  "type": "drupal-module",
  "license": "GPL-2.0-or-later",
  "homepage": "https://github.com/bentonow/bento-drupal-sdk",
  "support": {
    "issues": "https://github.com/bentonow/bento-drupal-sdk/issues",
    "source": "https://github.com/bentonow/bento-drupal-sdk"
  },
  "require": {
    "php": ">=8.1",
    "drupal/core": "^10.2",
    "guzzlehttp/guzzle": "^7.0"
  },
  "require-dev": {
    "drupal/core-dev": "^10.2"
  },
  "autoload": {
    "psr-4": {
      "Drupal\\bento_sdk\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Drupal\\Tests\\bento_sdk\\": "tests/src/"
    }
  },
  "extra": {
    "drupal": {
      "version": "0.8.1",
      "datestamp": "1753922267"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}